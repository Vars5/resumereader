<div class="container-fluid">
	
	<div class="row-fluid">
		
		<div class="span12 dashboardBar">
			<p><%= current_user.first_name%>'s resumeQuery Dasboard</p>


			<ul class="inline"<>
				<li><%= link_to '1. Write up your resume', resume_path%> </li>
				<li><%= link_to '2. Bring out your value'%> </li>
				<li><%= link_to '3. Review your resume'%> </li>
				<li><strike>4. View Companies (coming soon)</strike></li>	
				<li class="pull-right">Export your resume</li>
			</ul>
		</div>
	</div>
	
	<div class="row-fluid">
		
		<div class="span9">
			
			<div class="currentGoal">
				<div class="row-fluid">
					<div class="span3">
						<h5><i class="icon-star icon-white"></i> What's your current goal?</h5>
					</div>
					<div class="span9">
						<%= form_for :test do |f|%>
							<%= f.text_field :firstname %>
							<%= f.submit %>
						<% end %>
					</div>
				</div>
			</div>
			
			
		</div>
		
		<div class="span3">
			
			<div class="profileSpot">
				<h5>Completion Rate</h5>
				<div class="row-fluid">
					<div class="span5">
						<div class="circleWrapperSmall"></div>
					</div>
					<div class="span7">
						<p>90% Complete</p>
					</div>
				</div>
			</div>
			
			<div class="profileSpot">
				<h5>Did you get the interview? <span class="pull-right"><%= link_to 'Yes' %> &nbsp <%= link_to 'No'%></span></h5>
			</div>
			
			<div class="profileSpot">
				<h5>What's broken, hackable or suggestable?</h5>
				<%= form_for @problem, :as => :problems do |f| %>
					<%= f.text_area :problem, :placeholder => "We really appreciate what you think and find!" %>
					<%= f.hidden_field :reporter_id, :value => current_user.id %>
					<%= f.submit %>	
				<% end %>
				<%= link_to "Alternatively Visit our support page!", root_path, :class => "text-center" %>

			</div>
			
		</div>
		
		
		
	</div>
	
	
</div>