.dashboard

.row-fluid
	.offset3.span6
		.dashboard
		.center-alignment
			%h3 Invite a friend
			%br
		=form_for :invite, :url => {:controller => 'staticpages', :action => 'invitable'} do |f|
			=f.email_field :email, placeholder: "Email"
			=f.submit "Invite Person", :class => "sign-up-button"
	
	